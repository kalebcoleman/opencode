{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "tmux": {
    "enabled": true,
    "layout": "main-vertical",
    "main_pane_size": 70,
    "main_pane_min_width": 90,
    "agent_pane_min_width": 30
  },
  "background_task": {
    "defaultConcurrency": 6,
    "providerConcurrency": {
      "anthropic": 4,
      "openai": 4,
      "opencode": 6
    }
  },
  "agents": {
    "sisyphus": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
    },
    "hephaestus": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
    },
    "oracle": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
    },
    "librarian": {
      "model": "opencode/gpt-5-nano"
    },
    "explore": {
      "model": "opencode/gpt-5-nano"
    },
    "multimodal-looker": {
      "model": "opencode/glm-5-free"
    },
    "prometheus": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
    },
    "metis": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
    },
    "momus": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
    },
    "atlas": {
      "model": "opencode/glm-5-free"
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "opencode/gpt-5-nano"
    },
    "ultrabrain": {
      "model": "openai/gpt-5.3-codex",
      "variant": "xhigh"
    },
    "deep": {
      "model": "openai/gpt-5.3-codex"
    },
    "artistry": {
      "model": "openai/gpt-5.3-codex"
    },
    "quick": {
      "model": "opencode/gpt-5-nano"
    },
    "unspecified-low": {
      "model": "opencode/glm-5-free"
    },
    "unspecified-high": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
    },
    "writing": {
      "model": "opencode/glm-5-free"
    }
  },
  "_migrations": [
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex"
  ]
}
